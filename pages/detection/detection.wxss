/* 基因检测 */

.featuredPro {
  width: 100%;
  margin-bottom: 100rpx;
}

.fpHead {
  display: flex;
  justify-content: space-between;
  margin-bottom: 42rpx;
}

.fhTitle {
  height: 46rpx;
  line-height: 46rpx;
  border-left: 10rpx solid #333;
  color: #333;
  font-size: 36rpx;
  font-weight: 400;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.fhMore {
  margin-right: 20rpx;
  color: #333;
  font-size: 20rpx;
  font-weight: 400;
}

.fmImg {
  width: 10rpx;
  height: 16rpx;
}

.swiper-block {
  height: 480rpx;
  width: 100%;
}

.swiper-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  overflow: unset;
}

.slide-image {
  width: 380rpx;
  height: 390rpx;
  border-radius: 9rpx;
  box-shadow: 0px 0px 15px rgba(0, 0, 0, 0.3);
  margin: 0rpx 30rpx;
  z-index: 1;
}

.active {
  transform: scale(1);
  transition: all 0.2s ease-in 0s;
  height: 440rpx;
  z-index: 20;
  border-radius: 9rpx;
  box-shadow: 0px 0px 15px rgba(0, 0, 0, 0.3);
}

